{
  "name": "nodejs-basics",
  "version": "1.0.2",
  "description": "A basic nodejs server using minimum packages",
  "main": "src/app/index.js",
  "scripts": {
    "start": "node src/app",
    "serve": "nodemon",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "automate": "newman run src/api-tests/collections/v%npm_package_version%.json -e src/api-tests/environments/v%npm_package_version%.json",
    "automate:report": "newman run src/api-tests/collections/v%npm_package_version%.json -e src/api-tests/environments/v%npm_package_version%.json -r htmlextra --reporter-htmlextra-export src/api-tests/reports/v%npm_package_version%.html"
  },
  "keywords": [
    "node",
    "nodejs",
    "basics",
    "of",
    "nodejs"
  ],
  "author": "aAmanHussain",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.4.3",
    "newman": "^5.3.0",
    "newman-reporter-htmlextra": "^1.22.3",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2"
  }
}
